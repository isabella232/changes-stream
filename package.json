{
  "name": "@npmcorp/changes-stream",
  "version": "3.0.0",
  "description": "Stream changes from CouchDB via the /_changes api",
  "main": "index.js",
  "scripts": {
    "test:watch": "mocha --watch",
    "test": "mocha --exit",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "docker:fresh": "docker-compose down; docker-compose rm -f; docker-compose up -d couchdb",
    "docker:purge": "docker-compose down; docker-compose rm -f"
  },
  "files": [
    "package.json",
    "README.md",
    "index.js",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:npm/changes-stream.git"
  },
  "keywords": [
    "changes",
    "couchdb",
    "follow",
    "stream"
  ],
  "author": "npm <ops@npmjs.com>",
  "license": "MIT",
  "homepage": "https://github.com/npm/changes-stream",
  "dependencies": {
    "back": "^1.0.2",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.0",
    "normalize-registry-metadata": "^1.1.2",
    "standard": "^14.3.4"
  }
}
